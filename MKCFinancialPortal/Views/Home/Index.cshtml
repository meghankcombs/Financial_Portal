@section scripts {
    @if (TempData.ContainsKey("Message"))
    {
        <script>
            $(function () {
                swal('Thank you!', '@TempData["Message"]', 'success');
            });
        </script>
    }

    @if (TempData.ContainsKey("MessageForgot"))
    {
        <script>
                $(function () {
                    swal('Password Reset Confirmed!', '@TempData["MessageForgot"]', 'success');
                });
        </script>
    }
}
